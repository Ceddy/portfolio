div#home-container(hidden-home)
	div#home-graphic
		img#index-logo.index-graphic(src='/images/logo.png')
		img(icon-scrolled)#index-icon.index-graphic(src='/images/icon.png')
	div#home-info
		div
			// rarely still
			div
				p(ng-class="{focused: scroll >= 450}") RARELY 
				p(ng-class="{focused: scroll >= 450}") STILL
			// never bored
			div
				p(ng-class="{focused: scroll >= 500}") NEVER 
				p(ng-class="{focused: scroll >= 500}") BORED
			// always curious
			div
				p(ng-class="{focused: scroll >= 550}") ALWAYS
				p(ng-class="{focused: scroll >= 550}") CURIOUS 
	// CATTICUS ICONs