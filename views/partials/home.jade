div#home-container(hidden-home)
	div#home-graphic
		img#index-logo.index-graphic(src='/images/logo.png')
		img(icon-scrolled)#index-icon.index-graphic(src='/images/icon.png')
	div#home-meteor
		img(src='/images/meteor.png')
	div#home-info
		div
			p(ng-class="{focused: scroll >= 400 && scroll < 600}") hello hello {{scroll}}
			p.break(ng-class="{focused: scroll >= 500 && scroll < 700}") i am bushi {{scroll}}
		div
			p(ng-class="{focused: scroll >= 700 && scroll < 900}") developer {{scroll}}
			p.break(ng-class="{focused: scroll >= 800 && scroll < 1000}") & designer {{scroll}}
		div
			p.break(ng-class="{focused: scroll >= 1000 && scroll < 1200}") rarely still {{scroll}}
		div
			p.break(ng-class="{focused: scroll >= 1200 && scroll < 1400}") never bored {{scroll}}
		div	
			p.break(ng-class="{focused: scroll >= 1400 && scroll < 1600}") always curious {{scroll}}
	// CATTICUS ICONs